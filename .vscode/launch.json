    {
        "version": "0.2.0",
        "configurations": [
            {
                "name": "Debug: Ai Search Api",
                "type": "debugpy",
                "request": "launch",
                "module": "uvicorn",
                "args": [
                    "main:app",
                    "--reload",
                    "--port",
                    "8011"
                ],
                "console": "integratedTerminal",
                "env": {
                    "ENV": "loc"
                },
                "cwd": "${workspaceFolder}/src/cezzis_com_cocktails_aisearch",
                "serverReadyAction": {
                    "action": "openExternally",
                    "pattern": "Application startup complete.",
                    "uriFormat": "http://localhost:8011/scalar/v1"
                }
            }
        ]
    }