    {
        "version": "0.2.0",
        "configurations": [
            {
                "name": "Debug: Ai Search Api",
                "type": "debugpy",
                "request": "launch",
                "module": "uvicorn",
                "args": [
                    "src.cezzis_com_cocktails_aisearch.main:app",
                    "--reload"
                ],
                "console": "integratedTerminal",
                "env": {
                    "ENV": "local"
                },
                "serverReadyAction": {
                    "action": "openExternally",
                    "pattern": "INFO:     Uvicorn running on (http://127\\.0\\.0\\.1:[0-9]+) \\(Press CTRL\\+C to quit\\)",
                    "uriFormat": "%s/docs"
                }
            }
        ]
    }